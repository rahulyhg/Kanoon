<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="KextView">
        <attr name="fontType" format="enum">
            <enum name="regular" value="0"/>
            <enum name="black" value="1"/>
            <enum name="bold" value="2"/>
            <enum name="heavy" value="5"/>
            <enum name="thin" value="3"/>
            <enum name="light" value="4"/>
        </attr>
        <attr name="fontSpacing" format="enum">
            <enum name="normal" value="0"/>
            <enum name="title" value="1"/>
            <enum name="bigtitle" value="2"/>
        </attr>
    </declare-styleable>
</resources>